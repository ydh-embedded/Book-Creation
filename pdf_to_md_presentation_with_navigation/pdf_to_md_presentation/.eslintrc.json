{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "plugins": ["html"],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "script"
  },
  "rules": {
    "no-unused-vars": "warn"
  },
  "globals": {
    "pdfjsLib": "readonly",
    "Tesseract": "readonly"
  }
}